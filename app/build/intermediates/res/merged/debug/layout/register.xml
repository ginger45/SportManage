<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <LinearLayout
        android:id="@+id/widget181"
        android:layout_width="238dp"
        android:layout_height="58dp"
        android:orientation="vertical"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/widget183"
        android:layout_width="186dp"
        android:layout_height="78dp"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp">
        <Button
            android:id="@+id/widget189"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="注册"
            >
        </Button>
        <Button
            android:id="@+id/widget193"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="取消"
            >
        </Button>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/widget182"
        android:layout_width="208dp"
        android:layout_height="225dp"
        android:orientation="vertical"
        android:layout_marginTop="55dp"
        android:layout_below="@+id/widget181"
        android:layout_centerHorizontal="true">
        <EditText
            android:id="@+id/widget184"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:hint="学号"
            android:textSize="18sp"
            >
        </EditText>
        <EditText
            android:id="@+id/widget185"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:hint="姓名"
            android:textSize="18sp"
            >
        </EditText>
        <EditText
            android:id="@+id/widget186"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:hint="输入密码"
            android:textSize="18sp">
        </EditText>
        <EditText
            android:id="@+id/widget187"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:hint="确认密码"
            android:textSize="18sp"
            >
        </EditText>
        <EditText
            android:id="@+id/widget188"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:hint="班级"
            android:textSize="18sp"
            >
        </EditText>
    </LinearLayout>
</RelativeLayout>

